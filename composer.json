{
    "name": "gammadia/jms-metadata-1.x",
    "description": "Fork of jms/metadata 1.7.0 with support for modern PHP versions.",
    "keywords": ["annotations","metadata","yaml","xml"],
    "type": "library",
    "license": "MIT",
    "replace": {
        "jms/metadata": "^1.7"
    },
    "authors": [
        {
            "name": "Johannes M. Schmitt",
            "email": "schmittjoh@gmail.com"
        },
        {
            "name": "Asmir Mustafic",
            "email": "goetas@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2|^8.0"
    },
    "require-dev" : {
        "doctrine/cache" : "~1.0",
        "overtrue/phplint": "^3.0|^4.0|^5.0|^6.0",
        "phpstan/phpstan": "^1.8",
        "phpunit/phpunit": "^7.5|^8.5|^9.5",
        "symfony/cache" : "~3.1"
    },
    "autoload": {
        "psr-0": { "Metadata\\": "src/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.5.x-dev"
        }
    }
}
